{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "bun run build:fe",
    "beforeDevCommand": "bun run dev:fe",
    "devPath": "http://localhost:5173",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "DevUtils",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": false,
        "message": false,
        "open": true,
        "save": false
      },
      "fs": {
        "all": false,
        "copyFile": true,
        "createDir": false,
        "exists": true,
        "readDir": true
      },
      "http": {
        "all": false,
        "request": true,
        "scope": ["https://api.openai.com/*"]
      },
      "notification": {
        "all": true
      },
      "globalShortcut": {
        "all": true
      },
      "os": {
        "all": true
      },
      "clipboard": {
        "all": true,
        "writeText": true,
        "readText": true
      },
      "app": {
        "all": false,
        "hide": true,
        "show": true
      },
      "path": {
        "all": true
      },
      "protocol": {
        "all": false,
        "asset": false,
        "assetScope": ["http://*/*", "https://*/*"]
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": ["icons/icon_512x512.png", "icons/icon.icns"],
      "identifier": "com.dev-utils.desktop",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Dev Utils",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": ["https://Talljack.github.io/dev-utils/install.json"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IENGNjFGQzUwNUEyRTYxOEQKUldTTllTNWFVUHhoei93a0l6V1dPdGJJR0dBdDdOVWtCMHZiODFiY0t1cml6UDVYTzFpYlRUbTMK%"
    },
    "windows": [
      {
        "fullscreen": true,
        "height": 600,
        "resizable": true,
        "title": "Dev Utils",
        "width": 800
      }
    ],
    "systemTray": {
      "iconAsTemplate": true,
      "iconPath": "icons/icon_512x512.png"
    }
  }
}
